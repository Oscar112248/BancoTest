  "components": {
    "schemas": {
      "ActualizaClienteDto": {
        "type": "object",
        "properties": {
          "telefono": {
            "type": "string",
            "nullable": true
          },
          "direccion": {
            "type": "string",
            "nullable": true
          },
          "edad": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "ActualizarCuentaDto": {
        "type": "object",
        "properties": {
          "tipoCuenta": {
            "type": "string",
            "nullable": true
          },
          "estado": {
            "type": "boolean"
          },
          "eliminaCuenta": {
            "type": "boolean"
          },
          "clienteIdPersona": {
            "type": "integer",
            "format": "int64"
          }
        },
        "additionalProperties": false
      },
      "ActualizarEstadoCuentaDto": {
        "type": "object",
        "properties": {
          "estado": {
            "type": "boolean"
          }
        },
        "additionalProperties": false
      },
      "CrearClienteDto": {
        "required": [
          "codigoCliente",
          "contraseniaHash",
          "identificacion",
          "nombre"
        ],
        "type": "object",
        "properties": {
          "nombre": {
            "maxLength": 120,
            "minLength": 0,
            "type": "string"
          },
          "genero": {
            "pattern": "^[MFO]$",
            "type": "string",
            "nullable": true
          },
          "edad": {
            "maximum": 130,
            "minimum": 0,
            "type": "integer",
            "format": "int32",
            "nullable": true
          },
          "identificacion": {
            "maxLength": 20,
            "minLength": 0,
            "type": "string"
          },
          "direccion": {
            "maxLength": 250,
            "minLength": 0,
            "type": "string",
            "nullable": true
          },
          "telefono": {
            "maxLength": 25,
            "minLength": 0,
            "type": "string",
            "nullable": true
          },
          "codigoCliente": {
            "maxLength": 20,
            "minLength": 0,
            "type": "string"
          },
          "contraseniaHash": {
            "maxLength": 256,
            "minLength": 0,
            "type": "string"
          }
        },
        "additionalProperties": false
      },
      "CrearCuentaDto": {
        "type": "object",
        "properties": {
          "clienteIdPersona": {
            "type": "integer",
            "format": "int64"
          },
          "numeroCuenta": {
            "type": "string",
            "nullable": true
          },
          "tipoCuenta": {
            "type": "string",
            "nullable": true
          },
          "saldoInicial": {
            "type": "number",
            "format": "double"
          }
        },
        "additionalProperties": false
      },
      "DepositoDto": {
        "required": [
          "numeroCuenta"
        ],
        "type": "object",
        "properties": {
          "numeroCuenta": {
            "maxLength": 20,
            "minLength": 0,
            "type": "string"
          },
          "valor": {
            "minimum": 0,01,
            "type": "number",
            "format": "double"
          },
          "fecha": {
            "type": "string",
            "format": "date-time",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "RetiroDto": {
        "required": [
          "numeroCuenta"
        ],
        "type": "object",
        "properties": {
          "numeroCuenta": {
            "maxLength": 20,
            "minLength": 0,
            "type": "string"
          },
          "valor": {
            "minimum": 0,01,
            "type": "number",
            "format": "double"
          },
          "fecha": {
            "type": "string",
            "format": "date-time",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
